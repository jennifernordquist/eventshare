<h1>Listing settings</h1>

<table>
  <tr>
    <th>Require email</th>
    <th>Require phone</th>
    <th>Require rsvp</th>
    <th>Offer email</th>
    <th>Offer phone</th>
    <th>Offer rsvp</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @settings.each do |setting| %>
  <tr>
    <td><%= setting.require_email %></td>
    <td><%= setting.require_phone %></td>
    <td><%= setting.require_rsvp %></td>
    <td><%= setting.offer_email %></td>
    <td><%= setting.offer_phone %></td>
    <td><%= setting.offer_rsvp %></td>
    <td><%= link_to 'Show', setting %></td>
    <td><%= link_to 'Edit', edit_setting_path(setting) %></td>
    <td><%= link_to 'Destroy', setting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Setting', new_setting_path %>
